{
  "name": "@insurance-platform/backend",
  "version": "1.0.0",
  "description": "Backend monorepo for Multi-Vertical Insurance Lead Generation Platform",
  "private": true,
  "workspaces": [
    "services/*"
  ],
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "build": "lerna run build",
    "test": "lerna run test",
    "test:coverage": "lerna run test:coverage",
    "lint": "lerna run lint",
    "lint:fix": "lerna run lint:fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json}\"",
    "clean": "lerna clean",
    "prepare": "husky install",
    "security-audit": "npm audit && lerna exec -- npm audit",
    "type-check": "lerna run type-check",
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
  },
  "devDependencies": {
    "@commitlint/cli": "18.4.3",
    "@commitlint/config-conventional": "18.4.3",
    "@jest/globals": "29.0.0",
    "@jest/types": "29.6.0",
    "@opentelemetry/api": "1.4.0",
    "@opentelemetry/metrics": "1.0.0",
    "@opentelemetry/sdk-node": "0.45.1",
    "@types/compression": "1.7.5",
    "@types/cors": "2.8.16",
    "@types/express": "4.17.17",
    "@types/jest": "29.5.3",
    "@types/mock-http": "0.1.2",
    "@types/node": "20.5.0",
    "@types/validator": "13.11.6",
    "@typescript-eslint/eslint-plugin": "6.0.0",
    "@typescript-eslint/parser": "6.0.0",
    "eslint": "8.53.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.28.0",
    "eslint-plugin-jest": "27.2.0",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "jest-extended": "4.0.0",
    "jest-performance": "1.0.0",
    "lerna": "7.1.4",
    "lint-staged": "15.1.0",
    "mongodb-memory-server": "8.15.0",
    "nodemon": "3.0.1",
    "prettier": "3.0.0",
    "redis-mock": "0.56.3",
    "supertest": "6.3.0",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "typescript": "5.0.0"
  },
  "dependencies": {
    "axios": "1.6.0",
    "cache-manager": "5.2.3",
    "circuit-breaker-js": "0.8.0",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "dotenv": "16.3.1",
    "express": "4.18.2",
    "express-rate-limit": "7.1.4",
    "express-validator": "7.0.0",
    "helmet": "7.0.0",
    "http-status-codes": "2.3.0",
    "inversify": "6.0.1",
    "inversify-express-utils": "6.4.3",
    "ioredis": "5.0.0",
    "kafka-node": "5.0.0",
    "kafkajs": "2.2.0",
    "libphonenumber-js": "1.10.0",
    "mongoose": "7.5.0",
    "mongoose-field-encryption": "4.0.0",
    "opossum": "6.0.0",
    "passport-jwt": "4.0.1",
    "prom-client": "14.2.0",
    "rate-limiter-flexible": "2.4.1",
    "swagger-jsdoc": "6.2.8",
    "validator": "13.11.0",
    "winston": "3.10.0",
    "zod": "3.22.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
