{
  "name": "@insurance-platform/campaign-service",
  "version": "1.0.0",
  "description": "Campaign management microservice for the Multi-Vertical Insurance Lead Generation Platform",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "private": true,
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:perf": "jest-performance",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "prepare": "husky install",
    "migrate": "mongodb-migrate",
    "docs": "swagger-jsdoc"
  },
  "dependencies": {
    "express": "4.18.2",
    "mongoose": "7.5.0",
    "mongoose-field-encryption": "4.0.0",
    "winston": "3.10.0",
    "helmet": "7.0.0",
    "cors": "2.8.5",
    "compression": "1.7.4",
    "express-rate-limit": "7.1.4",
    "express-validator": "7.0.0",
    "dotenv": "16.3.1",
    "http-status-codes": "2.3.0",
    "inversify": "6.0.1",
    "inversify-express-utils": "6.4.3",
    "kafkajs": "2.2.0",
    "prom-client": "14.2.0",
    "rate-limiter-flexible": "2.4.1",
    "cache-manager": "5.2.3",
    "zod": "3.22.0",
    "ioredis": "5.0.0",
    "swagger-jsdoc": "6.2.8",
    "passport-jwt": "4.0.1",
    "mongodb-client-encryption": "2.3.0",
    "@opentelemetry/api": "1.4.0",
    "@opentelemetry/sdk-node": "0.45.1",
    "@opentelemetry/metrics": "1.0.0"
  },
  "devDependencies": {
    "typescript": "5.0.4",
    "@types/express": "4.17.17",
    "@types/node": "20.5.0",
    "@types/jest": "29.5.3",
    "@types/cors": "2.8.16",
    "@types/compression": "1.7.5",
    "@types/mock-http": "0.1.2",
    "@typescript-eslint/parser": "6.4.0",
    "@typescript-eslint/eslint-plugin": "6.4.0",
    "@jest/types": "29.6.0",
    "@jest/globals": "29.0.0",
    "jest": "29.6.2",
    "jest-extended": "4.0.0",
    "jest-performance": "1.0.0",
    "ts-jest": "29.1.1",
    "nodemon": "3.0.1",
    "ts-node": "10.9.1",
    "eslint": "8.47.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.28.0",
    "eslint-plugin-jest": "27.2.0",
    "prettier": "3.0.0",
    "husky": "8.0.3",
    "lint-staged": "15.1.0",
    "supertest": "6.3.0",
    "redis-mock": "0.56.3",
    "mongodb-memory-server": "8.15.0",
    "mongodb-migrate": "2.0.2"
  },
  "author": "Insurance Platform Team",
  "license": "UNLICENSED"
}